{
    "name": "contabo-music-downloader",
    "version": "1.0.0",
    "description": "Extrator de Músicas Contabo - Script Local",
    "main": "contabo-downloader.js",
    "type": "module",
    "bin": {
        "contabo-downloader": "./contabo-downloader.js"
    },
    "pkg": {
        "targets": [
            "node18-win-x64"
        ],
        "outputPath": "dist",
        "assets": [
            "contabo-config.json"
        ]
    },
    "scripts": {
        "build": "pkg .",
        "build-win": "pkg . --target node18-win-x64 --output dist/contabo-downloader.exe",
        "build-all": "pkg . --targets node18-win-x64,node18-linux-x64,node18-macos-x64"
    },
    "keywords": [
        "music",
        "downloader",
        "contabo",
        "mp3",
        "organizer"
    ],
    "author": "Plataforma Músicas",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.6.0",
        "fs-extra": "^11.1.1"
    },
    "devDependencies": {
        "pkg": "^5.8.1"
    }
}




